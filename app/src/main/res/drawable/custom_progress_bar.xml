<?xml version="1.0" encoding="utf-8"?>

<rotate android:fromDegrees="-90"
    android:pivotX="50%"
    android:pivotY="50%"
    android:toDegrees="270"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <shape
        android:shape="ring"
        android:useLevel="false">
        <gradient
            android:centerY="0.5"
            android:endColor="#00E7F0"
            android:startColor="#001D78"
            android:type="sweep"
            android:useLevel="false" />
    </shape>
</rotate>
